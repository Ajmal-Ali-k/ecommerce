    <script>
		function addToCart(Id){
    $.ajax({
        url:'/addToCart/'+Id,
        method:'get',
		success:(res)=>{
			if(res.cart){
				Swal.fire({
  		position: 'top-center',
  		icon: 'success',
  		title: 'Added to cart',
  		showConfirmButton: false,
  		timer: 1000
		})

			}
			else{
				location.href="/login"
			}
			console.log(res)
		}
    })
	}


	

	
