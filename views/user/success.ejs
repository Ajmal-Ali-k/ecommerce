                                                <%for(let i=0 ; i<order.products.length;i++) {%> 
                                                <tbody>
                                                    <tr>
                                                        <td width="20%">
                                                            <img src="<%= order.products[i].product.image[0] %>" width="50" />
                                                        </td>

                                                        <td width="60%">
                                                            <span class="font-weight-bold"><%= order.products[i].product.product_name %></span>
                                                            <div class="product-qty">
                                                                <span class="d-block" style=" color: black;">quantity : <%= order.products[i].quantity %></span>
                                                                <span>Color:<%= order.products[i].product.colour[0] %></span>
                                                            </div>
                                                        </td>
                                                        <td width="20%">
                                                            <div class="text-right">
                                                                <span class="font-weight-bold">$<%= order.products[i].totalprice%></span>
                                                            </div>
                                                        </td>
                                                    </tr>

                                            
