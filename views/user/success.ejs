                                                                <li><%= finalAddress.address%>,</li>
                                                                    <li><%= finalAddress.city%>, <%= finalAddress.state%>,</li>
                                                                    <li>  <%= finalAddress.pin%></li>
                                                                    <li>ph:<%= finalAddress.phone%></li>
                                                                   
                                                                  
                                                                  
                                                            </div>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>

                                        <div class="product border-bottom table-responsive">
